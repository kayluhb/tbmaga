{
  "name": "tbmaga",
  "author": "Caleb Brown <kayluhb@gmail.com> (http://kayluhb.com)",
  "version": "1.0.0",
  "description": "Travel blog",
  "homepage": "https://bitbucket.org/kayluhb/tbmaga#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/kayluhb/tbmaga.git"
  },
  "main": "bin/server.js",
  "scripts": {
    "build": "better-npm-run build",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "imagemin": "imagemin static/images static/images && imagemin static/*.png static && imagemin static/*.svg static && imagemin static/*.ico static",
    "lint": "eslint -c .eslintrc src",
    "lint:fix": "eslint -c .eslintrc src --fix",
    "postinstall": "npm run build",
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run",
    "start-prod": "better-npm-run start-prod",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
