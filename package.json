    "react-inline-css": "^2.0.0",
    "react-redux": "^4.4.4",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
    "redux-async-connect": "^1.0.0-rc4",
    "scroll-behavior": "^0.7.0",
    "serialize-javascript": "^1.1.2",
    "superagent": "^1.4.0",
