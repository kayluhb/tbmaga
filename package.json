    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.1",
    "classnames": "^2.2.4",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "file-loader": "^0.8.5",
    "gsap": "^1.18.2",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.12.0",
    "invariant": "^2.2.0",
    "lru-memoize": "^1.0.0",
    "map-props": "^1.0.0",
    "multireducer": "^2.0.0",
